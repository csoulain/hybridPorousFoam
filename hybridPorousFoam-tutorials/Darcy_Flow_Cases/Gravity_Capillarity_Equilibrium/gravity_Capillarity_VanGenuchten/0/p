FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "86";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    frontAndBack
    {
        type            empty;
    }
    top
    {
        type            totalPressure;
	p0 		uniform 0;
        value           uniform 0;
    }
    bottom
    {
        type            fixedFluxPressure;
    }
}


