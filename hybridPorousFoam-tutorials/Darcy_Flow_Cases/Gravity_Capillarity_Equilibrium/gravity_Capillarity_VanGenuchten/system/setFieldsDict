FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue epss 0.5
    volScalarFieldValue alpha.wetting 0
);

regions
(

     boxToCell //left boundary
    {
        box (0 0 0) (1 0.5 1);
        fieldValues
        (
	    volScalarFieldValue alpha.wetting 0.5
        );
    }

);


